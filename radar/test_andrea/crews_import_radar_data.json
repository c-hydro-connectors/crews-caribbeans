{
  "flags": {
    "clean_ancillary": false,
    "plot_figures": true,
    "compress_output": false
  },
  "name": "proj-crews",
  "version": "1.0.0",
  "template": {
    "datetime_file_in": "%Y%m%d%H%M",
    "datetime_folder_in": "%Y/%m/%d",
    "datetime_file_out": "%Y%m%d%H%M",
    "datetime_folder_out": "%Y/%m/%d"
  },
  "data": {
    "static": {
      "grid": "/home/silvestro/crews/data/static/radar_grid.txt",
      "grid_spacing": null
    },
    "dynamic":{
      "time": {
        "time_observed_period_h": 96
      },
      "input": {
        "folder": "/DATA/cimh_ago_sett_2021",
        "filename": "{datetime_file_in}**00dBZ.vol",
        "elevations_to_consider": [0,1],
        "limits_elevation_km": [70],
        "processing": {
          "ground_declutter": {
            "execute": true,
            "elev_diff_th": 5,
            "radial_velocity_th": 0.2
          },
          "z_to_r": {
            "a_coeff": 200,
            "b_coeff": 1.6
          }
        }
      },
      "output": {
        "folder": "/home/silvestro/crews/data/outcome/radar/{datetime_folder_out}",
        "filename_nc": "{datetime_file_out}_rainrate.nc",
	    "filename_tif": "{datetime_file_out}_rainrate.tif",
        "format": "tif"
      },
      "ancillary": {
        "folder": "/home/silvestro/crews/data/ancillary/radar/{datetime_folder_out}"
      }
    },
    "log": {
      "folder": "/home/silvestro/crews/log/",
      "filename": "crews_import_radar.log"
    }
  }
}

